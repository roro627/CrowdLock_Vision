{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "node_modules/.cache/tsbuild/node",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo/tsconfig.node.tsbuildinfo"
  },
  "include": ["vite.config.ts"]
}
